#include<stdio.h>
#include<stdlib.h>
#include<math.h>


int main()
{
	int i, n, a[1000]
	int sum, max;
	while (~scanf_s("%d", &n))
	{
		max = 0;
		sum = 0;
		if (n == 0)
			break;

		for (i = 0; i < n; i++)
		{
			scanf_s("%d", &a[i]);
		}
		for (i = 0; i < n; i++)
		{
			sum += a[i];
			if (sum > max)
			{
				max = sum;
			}
			if (sum < 0)
			{
				sum = 0;
			}

		}
		printf("最大连续子数组和：%d\n", max);

	}
	system("pause");
	return 0;
}
